{
  "name": "marketing-site",
  "main": "./src/workers/Site/SiteWorker.ts",
  "compatibility_date": "2025-06-01",
  "workers_dev": false,
  "preview_urls": true,
  "keep_vars": true,
  "minify": true,
  "assets": {
    "directory": "./site/",
    "run_worker_first": true,
    "not_found_handling": "404-page",
    "binding": "ASSETS"
  },
  "env": {
    "staging": {
      "main": "./src/workers/Site/SiteWorker.ts",
      "preview_urls": false,
      "workers_dev": false,
      "observability": {
        "enabled": true,
        "head_sampling_rate": 1
      }
    }
  },
  "observability": {
    "enabled": true,
    "head_sampling_rate": 0.02
  }
}
